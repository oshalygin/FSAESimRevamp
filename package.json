{
  "name": "fsae-sim-revamp",
  "version": "0.1.0",
  "description": "FSAESim Revamp",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node utilities/applicationStartMessage.js",
    "start": "npm-run-all --parallel open:server",
    "windows-start": "npm-run-all --parallel open-windows:server",
    "open:server": "babel-watch ./server/application.js",
    "open-windows:server": "babel-node ./server/application.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js 'webpack.config.*' 'client/**/*.js' 'client/**/*.jsx' 'server/**/*.js' 'test' './utilities' --color",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run tests -- --watch",
    "tests": "NODE_ENV=test mocha --reporter min",
    "windows-tests": "SET NODE_ENV=test && mocha --reporter min",
    "test": "NODE_ENV=test babel-node ./node_modules/.bin/isparta cover --include 'server/**/*.js*' --include 'client/**/*.js*' _mocha",
    "windows-test": "SET NODE_ENV=test && babel-node ./node_modules/isparta/bin/isparta cover --report html node_modules/mocha/bin/_mocha -- --reporter spec",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node utilities/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "windows-prebuild": "npm-run-all clean-dist windows-test lint build:html windows-buildjs",
    "build": "babel-node utilities/build.js",
    "windows-buildjs": "babel-node utilities/build.js",
    "windows-build": "npm-run-all windows-prebuild postbuild",
    "postbuild": "babel-node ./server/application.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oshalygin/FSAESimRevamp.git"
  },
  "keywords": [
    "formula",
    "fsae",
    "fsaesim",
    "simulation"
  ],
  "author": "oshalygin@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oshalygin/FSAESimRevamp/issues"
  },
  "homepage": "https://github.com/oshalygin/FSAESimRevamp#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "6.8.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "fitvids": "^2.0.0",
    "font-awesome": "^4.6.3",
    "image-webpack-loader": "^2.0.0",
    "jquery": "^2.2.4",
    "jquery.backstretch": "^2.1.13",
    "jquery.simple-text-rotator": "0.0.1",
    "jsx-loader": "^0.13.2",
    "magnific-popup": "^1.1.0",
    "material-design-lite": "^1.1.3",
    "mongoose": "^4.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "smoothscroll": "^0.2.2",
    "toastr": "^2.1.2",
    "uuid": "^2.0.2",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "babel-watch": "^2.0.2",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "coveralls": "^2.11.12",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "jsdom": "8.5.0",
    "jsdom-global": "^2.0.0",
    "mocha": "2.4.5",
    "mock-require": "^1.3.0",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "sinon": "^1.17.4",
    "style-loader": "0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "0.5.7",
    "wallaby-webpack": "0.0.22",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
