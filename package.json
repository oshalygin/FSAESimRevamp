{
  "name": "fsae-sim-revamp",
  "version": "1.0.0",
  "description": "FSAESim Revamp",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node ./utilities/startingDisplayMessage.js",
    "start": "npm-run-all --parallel open:server lint:watch",
    "open:server": "babel-watch ./server/application.js",
    "lint": "node_modules/.bin/esw webpack.config.* application server ./utilities",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "test": "NODE_ENV=test mocha --reporter spec utilities/testLoader.js \"application/**/*.spec.js\" \"server/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oshalygin/FSAESimRevamp.git"
  },
  "keywords": [
    "formula",
    "fsae",
    "fsaesim",
    "simulation"
  ],
  "author": "oshalygin@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oshalygin/FSAESimRevamp/issues"
  },
  "homepage": "https://github.com/oshalygin/FSAESimRevamp#readme",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.6",
    "jquery": "2.2.3",
    "mongoose": "^4.5.3",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.8.0",
    "babel-watch": "^2.0.2",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "rimraf": "2.5.2",
    "sinon": "^1.17.4",
    "style-loader": "0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
